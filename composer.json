{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "alexusmai/laravel-file-manager": "^2.2",
        "alkhachatryan/laravel-web-console": "^1.5",
        "axdlee/laravel-config-writer": "^1.0",
        "consoletvs/charts": "6.*",
        "fideloper/proxy": "~3.3",
        "filp/whoops": "^2.1",
        "ixudra/curl": "^6.16",
        "jackiedo/dotenv-editor": "1.*",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "orangehill/iseed": "^2.5",
        "thedevsaddam/laravel-schema": "^2.0",
        "yajra/laravel-datatables-oracle": "^8.8"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "HobbIoT\\": "plugin/HobbIoT/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan copy:console"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
